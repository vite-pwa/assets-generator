{
  "name": "@vite-pwa/assets-generator",
  "type": "module",
  "version": "0.0.11",
  "packageManager": "pnpm@8.11.0",
  "description": "Zero-config PWA Assets Generator",
  "author": "antfu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/vite-pwa/assets-generator#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vite-pwa/assets-generator.git"
  },
  "bugs": "https://github.com/vite-pwa/assets-generator/issues",
  "keywords": [
    "sharp",
    "sharp-ico",
    "pwa",
    "pwa icons generator",
    "pwa assets generator"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./api": {
      "import": {
        "types": "./dist/api.d.mts",
        "default": "./dist/api.mjs"
      },
      "require": {
        "types": "./dist/api.d.cts",
        "default": "./dist/api.cjs"
      }
    },
    "./config": {
      "import": {
        "types": "./dist/config.d.mts",
        "default": "./dist/config.mjs"
      },
      "require": {
        "types": "./dist/config.d.cts",
        "default": "./dist/config.cjs"
      }
    },
    "./presets/minimal": {
      "import": {
        "types": "./dist/presets/minimal.d.mts",
        "default": "./dist/presets/minimal.mjs"
      },
      "require": {
        "types": "./dist/presets/minimal.d.cts",
        "default": "./dist/presets/minimal.cjs"
      }
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "pwa-assets-generator": "./bin/pwa-assets-generator.mjs"
  },
  "files": [
    "bin",
    "dist",
    "presets"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "build": "unbuild && tsx scripts/postbuild.ts",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "run-playground": "pnpm -C playground run build",
    "test": "vitest"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "sharp": "^0.32.6",
    "sharp-ico": "^0.1.5",
    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@antfu/ni": "^0.21.12",
    "@types/debug": "^4.1.12",
    "@types/node": "^18.17.15",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.0.0-beta.6"
  }
}
