{
  "name": "@vite-pwa/assets-generator",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.6.8",
  "description": "Zero-config PWA Assets Generator",
  "author": "antfu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/vite-pwa/assets-generator#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vite-pwa/assets-generator.git"
  },
  "bugs": "https://github.com/vite-pwa/assets-generator/issues",
  "keywords": [
    "sharp",
    "sharp-ico",
    "pwa",
    "pwa icons generator",
    "pwa assets generator"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "default": "./dist/config.mjs"
    },
    "./presets/minimal": {
      "types": "./dist/presets/minimal.d.ts",
      "import": "./dist/presets/minimal.mjs",
      "default": "./dist/presets/minimal.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "pwa-assets-generator": "./bin/pwa-assets-generator.mjs"
  },
  "files": [
    "bin",
    "dist",
    "presets",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "pnpm -C test run test"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "colorette": "^2.0.20",
    "consola": "^3.1.0",
    "sharp": "^0.32.1",
    "sharp-ico": "^0.1.5",
    "unconfig": "^0.3.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@antfu/ni": "^0.21.3",
    "@types/debug": "^4.1.8",
    "@types/node": "^18.16.16",
    "bumpp": "^8.2.1",
    "eslint": "^8.42.0",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1"
  }
}
